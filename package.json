{
  "name": "file-store-tus",
  "version": "1.0.0",
  "description": "File store with TUS Protocol support for upload and download",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "postinstall": "node -e \"const fs=require('fs'),path=require('path');if(fs.existsSync('node_modules/tus-js-client/dist/tus.min.js')){fs.mkdirSync('public',{recursive:true});fs.copyFileSync('node_modules/tus-js-client/dist/tus.min.js','public/tus.min.js');console.log('TUS client copied to public/');}\""
  },
  "keywords": [
    "tus",
    "file-upload",
    "file-download",
    "resumable-upload"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tus/file-store": "^1.1.0",
    "@tus/server": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "tus-js-client": "^4.3.1"
  }
}
